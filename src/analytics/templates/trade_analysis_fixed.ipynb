 "cells": [
  {
   "cell_type": "markdown",
   "id": "cell-0",
   "metadata": {},
   "source": [
    "# Trade & Risk Analysis Notebook\\n",
    "\\n",
    "Analyzes trading performance through orders, fills, and position events."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "cell-1",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\\n",
    "import numpy as np\\n",
    "import json\\n",
    "from pathlib import Path\\n",
    "import matplotlib.pyplot as plt\\n",
    "import seaborn as sns\\n",
    "from datetime import datetime\\n",
    "\\n",
    "pd.set_option('display.max_columns', None)\\n",
    "pd.set_option('display.width', None)\\n",
    "plt.style.use('seaborn-v0_8-darkgrid')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "cell-2",
   "metadata": {
    "tags": ["parameters"]
   },
   "outputs": [],
   "source": [
    "# Parameters (for papermill)\\n",
    "run_dir = '.'\\n",
    "config_name = 'test'\\n",
    "symbols = ['SPY']\\n",
    "timeframe = '5m'"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "cell-3",
   "metadata": {},
   "source": [
    "## 1. Load Trace Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "cell-4",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Set the results directory\\n",
    "results_dir = Path(run_dir)\\n",
    "traces_dir = results_dir / 'traces'\\n",
    "\\n",
    "# Load trace files\\n",
    "traces = {}\\n",
    "\\n",
    "# Strategy signals\\n",
    "signals_path = list(traces_dir.rglob('signals/*/*.parquet'))\\n",
    "if signals_path:\\n",
    "    traces['signals'] = pd.read_parquet(signals_path[0])\\n",
    "    print(f\\"Loaded signals: {len(traces['signals'])} records\\")\\n",
    "\\n",
    "# Portfolio orders\\n",
    "orders_path = traces_dir / 'portfolio' / 'orders' / 'portfolio_orders.parquet'\\n",
    "if orders_path.exists():\\n",
    "    traces['orders'] = pd.read_parquet(orders_path)\\n",
    "    print(f\\"Loaded orders: {len(traces['orders'])} records\\")\\n",
    "\\n",
    "# Execution fills\\n",
    "fills_path = traces_dir / 'execution' / 'fills' / 'execution_fills.parquet'\\n",
    "if fills_path.exists():\\n",
    "    traces['fills'] = pd.read_parquet(fills_path)\\n",
    "    print(f\\"Loaded fills: {len(traces['fills'])} records\\")\\n",
    "\\n",
    "# Position events\\n",
    "pos_open_path = traces_dir / 'portfolio' / 'positions_open' / 'positions_open.parquet'\\n",
    "if pos_open_path.exists():\\n",
    "    traces['position_open'] = pd.read_parquet(pos_open_path)\\n",
    "    print(f\\"Loaded position opens: {len(traces['position_open'])} records\\")\\n",
    "\\n",
    "pos_close_path = traces_dir / 'portfolio' / 'positions_close' / 'positions_close.parquet'\\n",
    "if pos_close_path.exists():\\n",
    "    traces['position_close'] = pd.read_parquet(pos_close_path)\\n",
    "    print(f\\"Loaded position closes: {len(traces['position_close'])} records\\")"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "cell-5",
   "metadata": {},
   "source": [
    "## 2. Parse Metadata"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "cell-6",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Parse JSON metadata for all trace types\\n",
    "for trace_type, df in traces.items():\\n",
    "    if 'metadata' in df.columns and len(df) > 0:\\n",
    "        try:\\n",
    "            # Parse metadata\\n",
    "            metadata_parsed = df['metadata'].apply(lambda x: json.loads(x) if x else {})\\n",
    "            metadata_df = pd.DataFrame(list(metadata_parsed))\\n",
    "            \\n",
    "            # Add parsed columns to original dataframe\\n",
    "            for col in metadata_df.columns:\\n",
    "                if col not in df.columns:\\n",
    "                    df[col] = metadata_df[col]\\n",
    "            \\n",
    "            traces[trace_type] = df  # Update with parsed data\\n",
    "            print(f\\"Parsed {trace_type} metadata: {list(metadata_df.columns)[:10]}...\\")  # Show first 10 cols\\n",
    "        except Exception as e:\\n",
    "            print(f\\"Error parsing {trace_type} metadata: {e}\\")"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "cell-7",
   "metadata": {},
   "source": [
    "## 3. Order Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "cell-8",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Analyze orders and fills\\n",
    "if 'orders' in traces and 'fills' in traces:\\n",
    "    orders_df = traces['orders']\\n",
    "    fills_df = traces['fills']\\n",
    "    \\n",
    "    print(\\"=== ORDER AND FILL ANALYSIS ===\\")\\n",
    "    print(f\\"Total orders: {len(orders_df)}\\")\\n",
    "    print(f\\"Total fills: {len(fills_df)}\\")\\n",
    "    print(f\\"Fill rate: {len(fills_df)/len(orders_df)*100:.1f}%\\")\\n",
    "    \\n",
    "    # Analyze order sides\\n",
    "    if 'side' in orders_df.columns:\\n",
    "        print(\\"\\\\nOrder sides:\\")\\n",
    "        print(orders_df['side'].value_counts())\\n",
    "    \\n",
    "    # Analyze consecutive orders\\n",
    "    if 'side' in orders_df.columns:\\n",
    "        consecutive_same = 0\\n",
    "        max_consecutive = 0\\n",
    "        current_count = 1\\n",
    "        \\n",
    "        for i in range(1, len(orders_df)):\\n",
    "            if orders_df.iloc[i]['side'] == orders_df.iloc[i-1]['side']:\\n",
    "                current_count += 1\\n",
    "                max_consecutive = max(max_consecutive, current_count)\\n",
    "            else:\\n",
    "                if current_count > 1:\\n",
    "                    consecutive_same += 1\\n",
    "                current_count = 1\\n",
    "        \\n",
    "        print(f\\"\\\\nConsecutive orders of same side: {consecutive_same} sequences\\")\\n",
    "        print(f\\"Max consecutive: {max_consecutive}\\")\\n",
    "    \\n",
    "    # Estimate position changes\\n",
    "    position_changes = 0\\n",
    "    current_position = 0\\n",
    "    \\n",
    "    for i, row in orders_df.iterrows():\\n",
    "        side = row.get('side', '')\\n",
    "        if side == 'buy':\\n",
    "            if current_position <= 0:\\n",
    "                position_changes += 1\\n",
    "            current_position = 1\\n",
    "        elif side == 'sell':\\n",
    "            if current_position >= 0:\\n",
    "                position_changes += 1\\n",
    "            current_position = -1\\n",
    "    \\n",
    "    print(f\\"\\\\nEstimated position changes: {position_changes}\\")\\n",
    "else:\\n",
    "    print(\\"Missing orders or fills data\\")"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "cell-9",
   "metadata": {},
   "source": [
    "## 4. Trade Reconstruction from Orders/Fills"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "cell-10",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Reconstruct trades from fills\\n",
    "trades = []\\n",
    "\\n",
    "if 'fills' in traces:\\n",
    "    fills_df = traces['fills']\\n",
    "    \\n",
    "    # Track position and trades\\n",
    "    position = 0\\n",
    "    entry_price = None\\n",
    "    entry_bar = None\\n",
    "    \\n",
    "    for i, fill in fills_df.iterrows():\\n",
    "        side = fill.get('side', '')\\n",
    "        price = fill.get('price', 0)\\n",
    "        bar_idx = fill.get('idx', i)\\n",
    "        \\n",
    "        if side == 'buy':\\n",
    "            if position <= 0:  # Opening long\\n",
    "                if position < 0:  # Closing short first\\n",
    "                    if entry_price:\\n",
    "                        trades.append({\\n",
    "                            'entry_bar': entry_bar,\\n",
    "                            'exit_bar': bar_idx,\\n",
    "                            'entry_price': entry_price,\\n",
    "                            'exit_price': price,\\n",
    "                            'side': 'short',\\n",
    "                            'return_pct': (entry_price - price) / entry_price * 100\\n",
    "                        })\\n",
    "                entry_price = price\\n",
    "                entry_bar = bar_idx\\n",
    "            position = 1\\n",
    "        elif side == 'sell':\\n",
    "            if position >= 0:  # Opening short\\n",
    "                if position > 0:  # Closing long first\\n",
    "                    if entry_price:\\n",
    "                        trades.append({\\n",
    "                            'entry_bar': entry_bar,\\n",
    "                            'exit_bar': bar_idx,\\n",
    "                            'entry_price': entry_price,\\n",
    "                            'exit_price': price,\\n",
    "                            'side': 'long',\\n",
    "                            'return_pct': (price - entry_price) / entry_price * 100\\n",
    "                        })\\n",
    "                entry_price = price\\n",
    "                entry_bar = bar_idx\\n",
    "            position = -1\\n",
    "    \\n",
    "    if trades:\\n",
    "        trades_df = pd.DataFrame(trades)\\n",
    "        trades_df['bars_held'] = trades_df['exit_bar'] - trades_df['entry_bar']\\n",
    "        \\n",
    "        print(f\\"\\\\nReconstructed {len(trades_df)} trades from fills\\")\\n",
    "        print(\\"\\\\nTrade Summary:\\")\\n",
    "        print(trades_df[['side', 'bars_held', 'return_pct']].describe())\\n",
    "        \\n",
    "        # Performance metrics\\n",
    "        winning_trades = (trades_df['return_pct'] > 0).sum()\\n",
    "        total_trades = len(trades_df)\\n",
    "        win_rate = winning_trades / total_trades if total_trades > 0 else 0\\n",
    "        \\n",
    "        print(f\\"\\\\nWin rate: {win_rate:.1%}\\")\\n",
    "        print(f\\"Average return: {trades_df['return_pct'].mean():.2f}%\\")\\n",
    "        print(f\\"Total return: {trades_df['return_pct'].sum():.2f}%\\")\\n",
    "        \\n",
    "        # Plot returns\\n",
    "        plt.figure(figsize=(10, 5))\\n",
    "        plt.hist(trades_df['return_pct'], bins=30, alpha=0.7, edgecolor='black')\\n",
    "        plt.axvline(0, color='red', linestyle='--', alpha=0.5)\\n",
    "        plt.xlabel('Return (%)')\\n",
    "        plt.ylabel('Frequency')\\n",
    "        plt.title('Trade Return Distribution')\\n",
    "        plt.show()\\n",
    "    else:\\n",
    "        print(\\"No trades reconstructed from fills\\")"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "cell-11",
   "metadata": {},
   "source": [
    "## 5. Summary"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "cell-12",
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\\"=== SUMMARY ===\\")\\n",
    "\\n",
    "# Data summary\\n",
    "for trace_type, df in traces.items():\\n",
    "    print(f\\"{trace_type}: {len(df)} records\\")\\n",
    "\\n",
    "# Key insights\\n",
    "if 'orders' in traces and 'fills' in traces:\\n",
    "    orders_count = len(traces['orders'])\\n",
    "    fills_count = len(traces['fills'])\\n",
    "    \\n",
    "    print(f\\"\\\\nKey Insights:\\")\\n",
    "    print(f\\"- Fill rate: {fills_count/orders_count*100:.1f}%\\")\\n",
    "    \\n",
    "    if 'trades_df' in locals() and len(trades_df) > 0:\\n",
    "        print(f\\"- Total trades: {len(trades_df)}\\")\\n",
    "        print(f\\"- Win rate: {win_rate:.1%}\\")\\n",
    "        print(f\\"- Avg return per trade: {trades_df['return_pct'].mean():.2f}%\\")\\n",
    "\\n",
    "print(\\"\\\\nNote: Position events may be sparse if the strategy maintains positions for extended periods.\\")"
   ]
  }
